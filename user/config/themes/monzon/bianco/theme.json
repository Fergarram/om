{
	"base_size": "16px",
	"font_family": "system-ui",
	"font_sizes": {
		"ui": "var(--size-3)",
		"ui-lg": "var(--size-4)"
	},
	"global_classes": "text-ui",
	"colors": {
		"app_titlebar_fg": "#000000",
		"app_titlebar_bg": "#FFFFFF",
		"windows_titlebar_fg": "#000000",
		"windows_titlebar_bg": "#FFFFFF",
		"windows_statusbar_fg": "#000000",
		"windows_statusbar_bg": "#FFFFFF",
		"windows_content_bg": "#FFFFFF",
		"windows_content_fg": "#000000",
		"window_snap_preview_fg": "#FF00FF90",
		"window_snap_preview_bg": "#FF00FF10",
		"window_floating_border": "#000000",
		"window_snapped_border": "#FFFFFF",
		"select_backdrop_bg": "transparent",
		"select_bg": "#FFFFFF",
		"select_item_bg": "#FFFFFF",
		"select_item_fg": "#000000",
		"select_item_hover_bg": "#DEDEDE",
		"select_item_hover_fg": "#000000",
		"select_item_active_bg": "#DEDEDE",
		"select_item_active_fg": "#000000",
		"textfield_fg": "#000000",
		"textfield_bg": "#FFFFFF",
		"link": "#0000FF",
		"dim": "#808080",
		"hover": "#EEEEEE",
		"line": "#000000",
		"button_outline_border": "#E5E7EB",
		"file_browser_bg": "#FFFFFF",
		"file_browser_entry_hover": "#F3F4F6",
		"file_browser_icon_selected_bg": "#FFFFFF",
		"sound_viewer_bg": "#000000",
		"sound_viewer_fg": "#ffffff",
		"sound_viewer_wave": "#FF00FF"
	},
	"icons": {
		"font": true,
		"titilebar": {
			"file_browser_opened": "folder_open",
			"file_browser_closed": "folder_zip",
			"settings_launcher": "build",
			"assistant_launcher": "smart_toy",
			"window_minimize": "toast",
			"window_unmaximize": "toolbar",
			"window_maximize": "toolbar",
			"window_close": "disabled_by_default"
		},
		"file_browser": {
			"window": "folder_zip",
			"file": "draft",
			"folder_closed": "folder",
			"folder_opened": "folder_open",
			"trash": "delete",
			"prototype": "bolt",
			"project": "cyclone",
			"unknown": "unknown_document",
			"trash_dialog": "sd_card_alert"
		},
		"runner": {
			"arrow": "play_arrow"
		},
		"windows": {
			"window_close": "disabled_by_default",
			"menu_icon": "arrow_drop_down"
		},
		"combobox": {
			"arrow": "arrow_drop_down"
		},
		"select": {
			"check": "check"
		},
		"sound_viewer": {
			"window": "music_note",
			"pause": "pause",
			"play": "play_arrow",
			"repeat": "laps",
			"muted": "volume_off",
			"volume": "volume_up"
		},
		"image_viewer": {
			"window": "image_search",
			"animate": "slideshow"
		},
		"assistant": {
			"window": "smart_toy",
			"unsaved_warning": "sd_card_alert",
			"save": "sd_card",
			"send": "play_circle",
			"stop": "stop_circle",
			"dropdown": "expand_all"
		},
		"code_editor": {
			"window": "code",
			"unsaved_warning": "sd_card_alert",
			"assistant": "smart_toy",
			"save": "sd_card"
		},
		"settings": {
			"window": "build",
			"open_folder": "folder_open",
			"restart_warning": "warning",
			"select_wallpaper": "add_photo_alternate",
			"add_assistant": "add",
			"edit_instructions": "edit_square"
		}
	},
	"windows": {
		"resize_handles": {
			"edge_size": 12,
			"corner_size": 12,
			"offset": -6
		}
	},
	"editor": {
		"base": "vs",
		"basic_colors": {
			"background": "#FFFFFF",
			"selection": "#E8E8E8",
			"linenumber": "#AAAAAA",
			"inactive_selection": "#F0F0F0",
			"line_highlight": "#F8F8F8",
			"foreground": "#000000",
			"comment": "#FF00FF84",
			"foreground_dim": "#404040",
			"foreground_dimmer": "#606060"
		},
		"overrides": {
			"rules": null,
			"colors": null
		},
		"settings": {
			"minimap": { "enabled": false },
			"bracketPairColorization": { "enabled": false },
			"fontFamily": "Jetbrains Mono",
			"wordWrap": "on",
			"insertSpaces": false,
			"tabSize": 4,
			"fontSize": 12
		}
	},
	"components": {
		"titlebar": {
			"colors": "text-app-titlebar-fg bg-app-titlebar-bg",
			"layout": "relative top-0 left-0 w-full h-fit flex items-center justify-between p-1",
			"children_left": "flex items-center",
			"children_right": "flex items-center"
		},
		"window": {
			"snap-overlay": "fixed pointer-events-none z-[9999] border-2 border-window-snap-preview-fg bg-window-snap-preview-bg transition-all duration-150",
			"base": "absolute flex flex-col border-[1px] shadow-lg",
			"snapped": "border-window-snapped-border",
			"floating": "border-window-floating-border",
			"titlebar": "relative flex items-center justify-between p-1 w-full shrink-0 select-none cursor-default overflow-hidden bg-windows-titlebar-bg text-windows-titlebar-fg",
			"titlebar-handle": "grow h-full overflow-hidden",
			"menu_base": "text-dim flex gap-1 text-24 font-200 relative z-[1] bg-windows-titlebar-bg",
			"menu_right_click": "cursor-context-menu",
			"menu_arrow": "-ml-2 group-hover:text-windows-titlebar-fg",
			"title": "absolute left-1/2 top-1/2 -translate-x-1/2 -translate-y-1/2 w-fit max-w-full text-center whitespace-nowrap flex items-center justify-center",
			"titlebar_buttons": "flex items-center relative bg-windows-titlebar-bg",
			"content": "relative flex grow w-full bg-windows-content-bg text-windows-content-fg"
		},
		"textfield": {
			"base": "px-0.5",
			"variant_default": "border h-6 text-textfield-fg bg-textfield-bg disabled:bg-hover disabled:text-dim disabled:select-none disabled:pointer-events-none"
		},
		"button": {
			"base": "flex items-center justify-center gap-2 whitespace-nowrap focus-visible:outline-none disabled:pointer-events-none disabled:opacity-50 overflow-hidden",
			"size_default": "h-7 px-3 py-1.5",
			"size_icon": "h-6 w-6 min-w-6 min-h-6",
			"variant_default": "[&_icon]:text-24 [&_icon]:w-6 [&_icon]:h-6 [&_icon]:font-200 hover:bg-hover",
			"variant_outline": "[&_icon]:text-24 [&_icon]:w-6 [&_icon]:h-6 [&_icon]:font-200 border border-line hover:bg-hover",
			"variant_text": ""
		},
		"combobox": {
			"base": "relative",
			"input": "bg-transparent border-none px-0.5 w-full h-full outline-none focus:outline-none",
			"arrow": "absolute right-0 top-1/2 -translate-y-1/2 text-20 text-dim"
		},
		"dialog": {
			"base": "relative",
			"backdrop": "fixed inset-0 bg-black/20 flex items-center justify-center z-50",
			"variant_default": "bg-white text-black p-3 border border-black w-fit max-w-128 h-fit"
		},
		"select": {
			"backdrop": "fixed inset-0 z-50 bg-select-backdrop-bg",
			"layout": "absolute z-50 overflow-auto bg-select-bg shadow-lg ring-1 ring-black focus:outline-none",
			"item": "flex w-full cursor-pointer select-none items-center justify-between gap-2 px-1.5 py-1 text-select-item-fg bg-select-item-bg focus:bg-select-item-active-bg focus:text-select-item-active-fg [&_icon]:text-16",
			"item-disabled": "opacity-50 cursor-not-allowed pointer-events-none",
			"item-enabled": "hover:bg-select-item-hover-bg",
			"separator": "h-px bg-dim",
			"container": ""
		},
		"file_browser": {
			"content": "w-full h-full flex flex-col bg-file-browser-bg overflow-y-auto select-none",
			"entry": "w-full text-left flex items-center px-1 hover:bg-file-browser-entry-hover whitespace-nowrap hover:bg-hover",
			"entry_dragover": "bg-hover",
			"entry_text": "flex items-center",
			"entry_active": "font-bold",
			"entry_open": "italic",
			"entry_icon": "font-200 shrink-0 w-6 h-6 text-24 mr-1 not-italic",
			"entry_icon_selected": "invert bg-file-browser-icon-selected-bg"
		},
		"settings": {
			"heading": "font-bold text-ui-lg capitalize",
			"label": "text-ui capitalize",
			"fieldset": "w-full p-2 border border-line flex flex-col gap-1"
		}
	}
}
